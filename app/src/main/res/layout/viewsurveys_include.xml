<?xml version="1.0" encoding="utf-8"?>

<TableLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Survey ID:"
            android:textSize="24sp">

        </TextView>

        <TextView
            android:id="@+id/surveyidtext"
            android:layout_width="match_parent"
            android:text=""
            android:textSize="24sp">

        </TextView>

    </TableRow>
    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="District:"
            android:textSize="24sp">

        </TextView>

        <TextView
            android:id="@+id/districtidtext"
            android:layout_width="match_parent"
            android:text=""
            android:textSize="24sp">

        </TextView>

    </TableRow>
    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Locality:"
            android:textSize="24sp">

        </TextView>

        <TextView
            android:id="@+id/localityidtext"
            android:layout_width="match_parent"
            android:text=""
            android:textSize="24sp">

        </TextView>

    </TableRow>
    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="State:"
            android:textSize="24sp">

        </TextView>

        <TextView
            android:id="@+id/stateidtext"
            android:layout_width="match_parent"
            android:text=""
            android:textSize="24sp">

        </TextView>

    </TableRow>
    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="End Date:"
            android:textSize="24sp">

        </TextView>

        <TextView
            android:id="@+id/enddateidtext"
            android:layout_width="match_parent"
            android:text=""
            android:textSize="24sp">

        </TextView>

    </TableRow>
    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <Button
            android:id="@+id/submitbuttonidtext"
            android:layout_height="wrap_content"
            android:text="Take Survey"
            android:textSize="24sp"/>
    </TableRow>
</TableLayout>


